{
  "name": "unsecure-chatapp",
  "author": "RikThePixel",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "start:client": "vite client",
    "start:server": "nodemon server/index.ts",
    "start:build": "cd ./dist && node ./server/index.js",
    "start": "concurrently --kill-others \"npm run start:client\" \"npm run start:server\"",
    "build:client": "vite build client",
    "build:server": "tsc -p ./server",
    "build": "concurrently \"npm run build:client\" \"npm run build:server\""
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.42",
    "@vitejs/plugin-react": "^1.3.2",
    "concurrently": "^7.2.1",
    "nodemon": "^2.0.16",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "vite": "^2.9.12"
  },
  "dependencies": {
    "express": "^4.18.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  }
}