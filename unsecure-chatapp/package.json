  "scripts": {
    "start:client": "vite src/client",
    "start:server": "nodemon src/server/index.ts",
    "start:build": "node ./dist/server/index.js",
    "start": "concurrently --kill-others \"npm run start:client\" \"npm run start:server\"",
    "test:server": "cd ./src/server && ts-mocha 'test/**/*.ts'",
    "build:client": "vite build src/client",
    "build:server": "tsc -p ./src/server",
    "build": "concurrently \"npm run build:client\" \"npm run build:server\""
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/ws": "^8.5.3",
    "@vitejs/plugin-react": "^1.3.2",
    "chai": "^4.3.6",
    "concurrently": "^7.2.1",
    "nodemon": "^2.0.16",
    "react-router-dom": "^6.3.0",
    "sass": "^1.52.3",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "vite": "^2.9.12"
  },
  "dependencies": {
    "cors": "^2.8.5",
